{
  "name": "retail-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve retail-internet-banking",
    "s-retail-dev": "ng serve retail-internet-banking --configuration=development",
    "s-retail-mock": "ng serve retail-internet-banking --configuration=mock",
    "s-retail-sit": "ng serve retail-internet-banking --configuration=sit",
    "s-retail-uat": "ng serve retail-internet-banking --configuration=uat",
    "s-retail-production": "ng serve retail-internet-banking --configuration=production",
    "build-retail": "ng build retail-internet-banking",
    "build-retail-mock": "ng build retail-internet-banking --configuration=mock",
    "build-retail-sit": "ng build retail-internet-banking --configuration=sit",
    "build-retail-uat": "ng build retail-internet-banking --configuration=uat",
    "build-retail-production": "ng build retail-internet-banking --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.0",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@ngx-translate/core": "^16.0.0",
    "@ngx-translate/http-loader": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
